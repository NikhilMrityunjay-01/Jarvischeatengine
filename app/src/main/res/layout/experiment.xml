<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="200dp"
    android:layout_height="300dp"
    >

    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:visibility="gone"
        >
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:orientation="horizontal"
            android:background="#f7f7f7">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="JARVIS INT"
                android:textSize="10sp" />
        </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="15dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="SAFE"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="1"
            android:gravity="center"
            android:text="BRUTAL"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:text="EXTRA"
            android:textSize="10sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/safe"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="gone">

        <TextView
            android:id="@+id/textView0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Switch
            android:id="@+id/switch2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Wide view" />
        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Aimbot" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Small croshair" />
        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   No fog" />
        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Less recoil" />
        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Auto headshot" />

        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Anti shake" />
        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Aimbot 200m" />
        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/brutal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="gone">
        <TextView

            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Switch
            android:id="@+id/switch20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Auto headshot" />
        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch21"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Flash" />
        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch22"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Sit scope" />
        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch23"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Stand scope" />
        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch24"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Long jump" />
        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch25"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Fast parachute" />
        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch26"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Fly all vehicles" />
        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch27"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Magic bullet" />
        <TextView

            android:layout_width="match_parent"
            android:layout_height="2dp" />

        <Switch
            android:id="@+id/switch28"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Aimbot" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/extra"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="gone">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Switch
            android:id="@+id/switch29"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Antiseal" />

        <Switch
            android:id="@+id/switch30"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Bypass 3rd party" />

        <Switch
            android:id="@+id/switch31"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Protect ++" />

        <Switch
            android:id="@+id/switch32"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="   Esp" />
    </LinearLayout>

</LinearLayout>

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/relativeLayoutParent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="UselessParent">

    <RelativeLayout
        android:id="@+id/collapsedView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:visibility="visible">

        <ImageView
            android:id="@+id/img"
            android:layout_width="24dp"
            android:layout_height="22dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/injection" />
    </RelativeLayout>
    </RelativeLayout>
</FrameLayout>